{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": null,
  "framework": null,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/reminders",
      "dest": "/api/reminders.ts"
    },
    {
      "src": "/api/check-reminders",
      "dest": "/api/check-reminders.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.ts"
    }
  ]
}